    </nav>
  </div>
</div>

    <div id="menu-cover"></div>
  </div>

  <!-- Main header section -->
  <div id="home-header-background"></div>
  <div id="home-header">
    <div id="header-text">
      <h1>Create interactive maps</h1>
      <h2>Collaborate. Organize your geo-data.<br>Embed maps on your website.</h2>
      <a class="try-button" href="http://localhost:8080/explore">Explore maps</a>
    </div>
  </div>

  <!-- create section -->
  <div id="promo-1">
