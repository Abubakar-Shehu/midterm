        <p>It's as easy as pie</p>
        <a class="createnow" href="http://localhost:8080/create">CREATE MAP NOW</a>

      </div>
      <div class="promo-img">
        <img src="/images/2.jpg" alt="MapMap2" />
      </div>
    </div>
  </div>

  <!-- explore section -->
 <div id="explore">
  <div class="explore-inner">
    <div class="explore-text">
      <h1>See some of our community made maps here</h1>
      <p>Have a link of your own? Paste it here</p>
      <form action="/search" method="GET" class="search-form">
  <input
    type="text"
    name="q"
    placeholder="Search..."
    aria-label="Search"
    required
  />
  <button type="submit">Search</button>
</form>
    </div>
  </div>
</div>

<!-- Maps JavaScript file -->
<script src="/scripts/maps2.js"></script>
<!-- Maps container with 4 columns -->
<div id="maps-container" class="maps-container">
  <div class="map">
    <div class="map-overlay">Map 1</div>
    <div id="map1" class="map-inner"></div>
  </div>
  <div class="map">
    <div class="map-overlay">Map 2</div>
    <div id="map2" class="map-inner"></div>
  </div>
  <div class="map">
